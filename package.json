    "stylelint-config-standard": "^32.0.0"
  },
  "dependencies": {
    "gsap": "^3.9.1",
    "lodash": "^4.17.21",
    "modern-normalize": "^1.1.0",
    "nouislider": "^15.5.1",
    "swiper": "^9.0.1"
